<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"             
             xmlns:Properties="clr-namespace:SkyEditor.RomEditor.Avalonia.Properties;assembly=SkyEditor.RomEditor.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SkyEditor.RomEditor.Avalonia.Views.Rtdx.RtdxRomView">
  <TabControl TabStripPlacement="Left" Margin="0,0,0,0">
    <TabItem Header="{x:Static Properties:Resources.Views_Rtdx_RtdxRomView_StartersCollection}">
      <ContentControl Content="{Binding StartersCollection}" />
    </TabItem>
    <TabItem Header="{x:Static Properties:Resources.Views_Rtdx_RtdxRomView_DungeonsCollection}">
      <ContentControl Content="{Binding DungeonsCollection}" />
    </TabItem>
  </TabControl>
</UserControl>
